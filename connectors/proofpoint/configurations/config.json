{
    "connection": {
        "schedule": {
            "default": "0 */1 * * *",
            "type": "cron",
            "previous": ["frequency", "time"],
            "options": ["0 */1 * * *","0 */2 * * *","0 */4 * * *", "0 */6 * * *", "0 */12 * * *", "mm HH * * *"]
        },
        "type": {
            "type": "connectorType",
            "displayName": "Proofpoint",
            "group": "proofpoint"
        },
        "host": {
            "type": "text",
            "regex": "^(([a-zA-Z0-9]|[a-zA-Z0-9][a-zA-Z0-9_:/\\-]*[a-zA-Z0-9])\\.)*([A-Za-z0-9]|[A-Za-z0-9][A-Za-z0-9_:/\\-]*[A-Za-z0-9])$|^(([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])\\.){3}([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])$",
            "previous": "configuration.auth.proofpoint_url"
        }
    },
    "configuration": {
        "auth": {
            "type": "fields",
            "principle": {
                "type": "password"
            },
            "secret": {
                "type": "password"
            }
        }
    }
}
