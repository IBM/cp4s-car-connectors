{
    "connection": {
        "type": {
            "type": "connectorType",
            "displayName": "Red Hat Advanced Cluster Security for Kubernetes (StackRox)",
            "group": "rhacs"
        },
        "host": {
            "type": "text",
            "regex": "^(([a-zA-Z0-9]|[a-zA-Z0-9][a-zA-Z0-9\\-]*[a-zA-Z0-9])\\.)*([A-Za-z0-9]|[A-Za-z0-9][A-Za-z0-9\\-]*[A-Za-z0-9])$"
        },
        "selfSignedCert": {
            "type": "password",
            "optional": true
        },
        "sni": {
            "type": "text",
            "optional": true
        }
    },
    "configuration": {
        "auth": {
            "type": "fields",
            "token": {
                "type": "password"
            }
        }
    }
}