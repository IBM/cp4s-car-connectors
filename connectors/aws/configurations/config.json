{
    "connection": {
        "schedule": {
            "default": "0 */4 * * *",
            "type": "cron",
            "previous": ["frequency", "time"],
            "options": ["0 */4 * * *", "0 */6 * * *", "0 */12 * * *", "mm HH * * *"]
        },
        "type": {
            "type": "connectorType",
            "displayName": "AWS"
        },
        "region": {
            "type": "text"
        }
    },
    "configuration": {
        "auth": {
            "type": "fields",
            "account_id": {
                "type": "password"
            },
            "aws_access_key_id": {
                "type": "password"
            },
            "aws_secret_access_key": {
                "type": "password"
            }
        }
    }
}
