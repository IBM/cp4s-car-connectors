        "help": {
            "default": "data-sources-car-{CONNECTOR}.html",
            "type": "link"
        },
        "image": {
            "type": "text",
            "hidden": true,
            "default": "{image_link}"
        },
        "type": {
            "maxConnections": 1,
            "maxConfigurations": 1
        }
    }
}
