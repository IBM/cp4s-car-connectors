            "previous": ["frequency", "time"],
            "options": ["*/5 * * * *", "*/10 * * * *", "*/15 * * * *", "*/30 * * * *", "0 */1 * * *","0 */2 * * *","0 */4 * * *", "0 */6 * * *", "0 */12 * * *", "mm HH * * *"]
